{
    "name": "drupal/legacy-project",
    "description": "Project template for Drupal 8 projects with composer following drupal/drupal layout",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "christian-riesen/otp": "^2.7",
        "composer/installers": "^1.9",
        "cweagans/composer-patches": "^1.7",
        "defuse/php-encryption": " ^2.2",
        "drupal/acquia_connector": "^4.0",
        "drupal/acquia_purge": "^1.2",
        "drupal/acquia_search": "^3.1",
        "drupal/addtoany": "^2.0",
        "drupal/admin_toolbar": "^3.1",
        "drupal/autologout": "^1.4",
        "drupal/better_exposed_filters": "^6.0",
        "drupal/block_field": "1.x-dev@dev",
        "drupal/block_ipaddress": "^1.0",
        "drupal/bootstrap_barrio": "^5.5",
        "drupal/bootstrap_sass": "^5.0",
        "drupal/chosen": "^4.0",
        "drupal/ckeditor_indentblock": "^1.0",
        "drupal/clamav": "^2.0",
        "drupal/color": "^1.0",
        "drupal/config_ignore": "^3.2",
        "drupal/config_pages": "^2.16",
        "drupal/core-composer-scaffold": "^10.1",
        "drupal/core-project-message": "^10.1",
        "drupal/core-recommended": "^10.1",
        "drupal/core-vendor-hardening": "^10.1",
        "drupal/devel": "^5.0",
        "drupal/devel_kint_extras": "^1.1",
        "drupal/dynamic_entity_reference": "^3.0",
        "drupal/embed": "^1.6",
        "drupal/entity_browser": "^2.9",
        "drupal/entity_embed": "^1.4",
        "drupal/entity_reference_tree": "^2.3",
        "drupal/entity_update": "^3.0",
        "drupal/externalauth": "^2.0",
        "drupal/extlink": "^1.6",
        "drupal/field_group": "^3.1",
        "drupal/field_states_ui": "^3.0",
        "drupal/ga4_google_analytics": "^1.1",
        "drupal/geolocation": "^3.12",
        "drupal/gtm": "^1.6",
        "drupal/hierarchical_term_formatter": "^1.3",
        "drupal/linkit": "^6.1@RC",
        "drupal/linkit_media_library": "^1.0",
        "drupal/material_icons": "^2.0",
        "drupal/media_directories": "^2.1@beta",
        "drupal/media_entity_file_replace": "^1.0",
        "drupal/menu_block": "^1.8",
        "drupal/menu_breadcrumb": "^2.0@alpha",
        "drupal/metatag": "^1.15",
        "drupal/migrate_devel": "^2.3",
        "drupal/migrate_plus": "^6.0",
        "drupal/migrate_tools": "^6.0",
        "drupal/moderated_content_bulk_publish": "^2.0",
        "drupal/mysql56": "^1.6",
        "drupal/paragraphs": "^1.12",
        "drupal/password_policy": "^4.0",
        "drupal/path_redirect_import": "^2.0",
        "drupal/pathauto": "^1.8",
        "drupal/pdfpreview": "^1.1",
        "drupal/phpexcel": "^4.0@beta",
        "drupal/preview_link": "^2.1@alpha",
        "drupal/purge_file": "^1.1",
        "drupal/quick_node_clone": "^1.16",
        "drupal/rdf": "^2.0",
        "drupal/real_aes": "^2.4",
        "drupal/rebuild_cache_access": "^1.9",
        "drupal/redirect": "1.x-dev@dev",
        "drupal/remove_http_headers": "^2.0",
        "drupal/require_login": "^3.0",
        "drupal/samlauth": "^3.8",
        "drupal/scanner": "^1.0@RC",
        "drupal/scheduler": "^2.0@RC",
        "drupal/scheduler_content_moderation_integration": "^2.0@beta",
        "drupal/search_api": "^1.28",
        "drupal/search_api_solr": "^4.2",
        "drupal/seckit": "^2.0",
        "drupal/shield": "^1.4",
        "drupal/sitemap": "^2.0@beta",
        "drupal/social_media_links": "^2.9",
        "drupal/stage_file_proxy": "^2.0",
        "drupal/structure_sync": "^2.0",
        "drupal/svg_image": "^3.0",
        "drupal/term_reference_tree": "^2.0",
        "drupal/tfa": "^2.0@alpha",
        "drupal/tinypng": "^1.2",
        "drupal/twig_tweak": "^3.2",
        "drupal/ultimate_cron": "^2.0@alpha",
        "drupal/upgrade_status": "^4.0",
        "drupal/username_enumeration_prevention": "^1.2",
        "drupal/views_bulk_operations": "^4.2",
        "drupal/views_sort_null_field": "^1.3",
        "drupal/vite": "^1.2",
        "drupal/xmlsitemap": "^1.0",
        "drush/drush": "^11",
        "kint-php/kint": "^4.2",
        "symfony/dependency-injection": "^6.3",
        "symfony/http-kernel": "^6.2",
        "symfony/var-dumper": "^6.2",
        "twig/twig": "^3.6"
    },
    "require-dev": {
        "drupal/coder": "^8.3",
        "drupal/core-dev": "^10",
        "ergebnis/composer-normalize": "^2.30",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "drupal/console-extend-plugin": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "drupal/core-vendor-hardening": true,
            "ergebnis/composer-normalize": true,
            "simplesamlphp/composer-module-installer": false,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "drupal-scaffold": {
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            },
            "locations": {
                "web-root": "docroot/"
            }
        },
        "installer-paths": {
            "docroot/core": ["type:drupal-core"],
            "docroot/libraries/{$name}": ["type:drupal-library"],
            "docroot/modules/contrib/{$name}": ["type:drupal-module"],
            "docroot/profiles/contrib/{$name}": ["type:drupal-profile"],
            "docroot/themes/contrib/{$name}": ["type:drupal-theme"],
            "docroot/drush/Commands/contrib/{$name}": ["type:drupal-drush"],
            "docroot/modules/custom/{$name}": ["type:drupal-custom-module"],
            "docroot/themes/custom/{$name}": ["type:drupal-custom-theme"]
        },
        "drupal-core-project-message": {
            "include-keys": ["homepage", "support"],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youï¿½ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/legacy-project template!               </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",

                "  * Install the site: https://www.drupal.org/docs/8/install",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        },
        "enable-patching": true,
         "patches": {
            
        	"drupal/core": {
            	
            	"purge module tag cache minify a": "docroot/patches/purge.patch"
        	}
        },
        "patches-ignore": {
             "drupal/field_states_ui": {
                "Not working with entity_reference_autocomplete:3196272": "https://www.drupal.org/files/issues/2021-06-08/3196272-3.patch"
            },
             "drupal/password_policy": {
            	"Some mandatory parameters missing WSOD:3313959": "https://www.drupal.org/files/issues/2022-10-17/3313959-3-password_policy-edit-form-path.patch"
        	}
        }
    },
    "scripts": {
        "post-drupal-scaffold-cmd": [
            "patch -p1 < docroot/patches/htaccess.patch"
        ]
    }
}

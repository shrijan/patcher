{
    "name": "drupal/vite",
    "description": "Vite integration for Drupal asset libraries.",
    "license": "GPL-2.0-or-later",
    "type": "drupal-module",
    "authors": [
        {
            "name": "wotnak",
            "email": "wotnak@pm.me",
            "homepage": "https://www.drupal.org/u/wotnak",
            "role": "Maintainer"
        }
    ],
    "homepage": "https://drupal.org/project/vite",

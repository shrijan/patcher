        "phpstan/phpstan-strict-rules": "^1.5",
        "phpunit/phpunit": "^9.6",
        "slevomat/coding-standard": "^8.14",
        "squizlabs/php_codesniffer": "^3.7",
        "vincentlanglet/twig-cs-fixer": "^1.6"
